import "hashes/sha256/512bitPadded" as sha256;

def merkleRoot(u32[8] leaf, u32[14][8] path, bool[14] path_directions) -> u32[8] {
    u32[8] mut current = leaf;

    for u32 i in 0..14 {
        current = if path_directions[i] {
            sha256(current, path[i])
        } else {
            sha256(path[i], current)
        };
    }

    return current;
}


def main() -> bool {
    return true;
}